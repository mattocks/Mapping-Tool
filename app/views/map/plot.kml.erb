<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
<% @features.nodes.each do |node| %>  <Placemark>
<% unless node.user.nil? %>    <name><%= node.user %></name><% end %>
    <Point>
      <coordinates><%= node.lon %>,<%= node.lat %></coordinates>
    </Point>
  </Placemark>
<% end %>
</Document>
</kml>